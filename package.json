    "tracker"
  ],
  "author": "OSU Open Source Lab <support@osuosl.org>",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/tschuy/timesync/issues"
  },
  "dependencies": {
    "knex": "^0.7.6",
    "sqlite3": "3.0.5",
    "koa": "^0.19.0",
    "koa-knex": "0.7.5",
    "koa-route": "2.4.0"
  },
  "devDependencies": {
    "mocha": "^2.2.4",
    "mocha": "^1.12.0",
    "jshint": "^2.6.3"
  }
}
